{
  "name": "test-timer",
  "version": "1.0.0",
  "description": "test",
  "main": "dist/index.js",
  "scripts": {
    "watch": "nodemon --watch './**/*.{ts}' --exec 'node --experimental-specifier-resolution=node --loader ts-node/esm' src/index.ts",
    "client": "npm run start --prefix client",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\" \"npm run client\"",
    "build": "babel src -d dist"
  },
  "keywords": [
    "test-timer"
  ],
  "author": "Vlad Gareev",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.17",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ts-node": "^10.9.1"
  },
  "exports": "./dist/index.js"
}
